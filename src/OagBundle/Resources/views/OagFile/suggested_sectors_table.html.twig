<table class="pure-table pure-table-bordered">
    <thead>
        <tr>
            <th>AGROVOC Code</th>
            <th>Description</th>
            <th>Confidence</th>
        </tr>
    </thead>
    <tbody>
        {% for sector in sectors %}
            <tr>
                {{ include('OagBundle::OagFile/sector_row.html.twig', { 'sector': sector.sector }) }}
                <td>{{ sector.confidence }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
